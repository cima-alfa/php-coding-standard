{
    "name": "jeremy-step/php-stylish",
    "description": "Custom PHP CS by Jeremy Stepanek... Inspired by Nette Coding Standard and Laravel Pint.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Stepanek",
            "email": "contact@sjeremy.dev",
            "homepage": "https://sjeremy.dev"
        }
    ],
    "require": {
        "php": "^8.4",
        "friendsofphp/php-cs-fixer": "^3.84",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.31",
        "nette/neon": "^3.4.4",
        "nette/tester": "^2.5",
        "tracy/tracy": "^2.10.10"
    },
    "autoload": {
        "psr-4": {
            "PHPStylish\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.4.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
